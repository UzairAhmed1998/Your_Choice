@model IEnumerable<Admin_panel.Models.Data.Contact>

@{
    ViewData["Title"] = "CustomerMessages";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Customer Messages</h3>


<table class="table">
    <thead>
        <tr>
            <th>
              Customer Name
            </th>
            <th>
              Customer Email
            </th>
            <th>
               Customer Message
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.c_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.c_email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.c_msg)
            </td>
            <td>
              
               
                <a class="btn btn-danger" asp-action="MDelete" asp-controller="Home" asp-route-id="@item.c_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
